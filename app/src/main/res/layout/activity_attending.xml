<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Attending">

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="בקשות מיוחדות"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.881"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/helloTextPrivateProfile3"
        app:layout_constraintVertical_bias="0.116" />

    <Button
        android:id="@+id/button4"
        android:layout_width="102dp"
        android:layout_height="46dp"
        android:text="חיול מהיר"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button3"
        app:layout_constraintHorizontal_bias="0.278"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/helloTextPrivateProfile3"
        app:layout_constraintVertical_bias="0.116" />

    <TextView
        android:id="@+id/helloTextPrivateProfile2"
        android:layout_width="165dp"
        android:layout_height="42dp"
        android:text="שלום, אדון קורונה."
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/attendingTitle"
        app:layout_constraintVertical_bias="0.035" />

    <TextView
        android:id="@+id/helloTextPrivateProfile3"
        android:layout_width="125dp"
        android:layout_height="44dp"
        android:layout_marginEnd="24dp"
        android:text="צפי שעת הגעה:"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="@+id/privateProfileTitle"
        app:layout_constraintTop_toBottomOf="@+id/helloTextPrivateProfile2"
        app:layout_constraintVertical_bias="0.042" />

    <TextView
        android:id="@+id/attendingTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="אישור התייצבות"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022" />

    <Spinner
        android:id="@+id/chooseOptions"
        android:layout_width="213dp"
        android:layout_height="45dp"
        android:layout_marginBottom="552dp"
        android:spinnerMode="dropdown"
        app:layout_constraintBottom_toBottomOf="@+id/helloTextPrivateProfile3"
        app:layout_constraintEnd_toStartOf="@+id/helloTextPrivateProfile3"
        app:layout_constraintHorizontal_bias="0.824"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/helloTextPrivateProfile3"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>